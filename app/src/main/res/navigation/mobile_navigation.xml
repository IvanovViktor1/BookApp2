<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.bookapp2.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_search"
        android:name="com.example.bookapp2.ui.searcH.SearchFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_for_search" />

    <fragment
        android:id="@+id/navigation_grades"
        android:name="com.example.bookapp2.ui.grades.GradesFragment"
        android:label="@string/title_grades"
        tools:layout="@layout/fragment_for_grades" />

    <fragment
        android:id="@+id/navigation_setting"
        android:name="com.example.bookapp2.ui.setting.SettingFragment"
        android:label="@string/title_setting"
        tools:layout="@layout/fragment_for_setting" />

</navigation>